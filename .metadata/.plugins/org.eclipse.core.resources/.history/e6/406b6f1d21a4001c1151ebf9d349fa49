package day0315;

public class Ex6Operator {

	public static void main(String[] args) {
		
		
		
		int a = 5, b = 5, x, y;
		x = a++;
		y = ++b;
		
		System.out.println(a + "," + b);
		System.out.println(x + "," + y);	// x = 5, y = 6 왜? ++이 뒤에붙으면 출력되고 증가, 앞에붙으면 출력되기전에 증가함.
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		

	}

}
